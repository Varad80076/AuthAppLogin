{
  "version": 2,
  "builds": [
      {
          "src": "index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "index.js"
      },
      {
          "src": "/",
          "dest": "/auth/signup"
      },
      {
          "src": "/login",
          "dest": "/auth/signup"
      },
      {
          "src": "/next",
          "dest": "/next"
      }
  ]
      
}