{
    "version": 2,
    "name": "auth-app-login",
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node",
        "config": {
          "maxDuration": 300,
          "runtime": "nodejs18.x",
          "regions": ["iad1"]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/auth/signup",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/auth/login",
        "dest": "/api/index.js"
      }
    ],
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://auth-app-sepia-pi.vercel.app"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, OPTIONS, PUT, DELETE"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type, Authorization"
          }
        ]
      }
    ]
  }
  