{
    "version": 2,
    "name": "auth-app-login",
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node",
        "config": {
          "maxDuration": 300,
          "runtime": "nodejs18.x",
          "regions": ["iad1"]
        }
      }
    ],
    "routes": [
        
       {
        "src": "/api/auth/signup",
        "dest": "/api/index.js",
        "headers": {
          "Access-Control-Allow-Origin": "https://auth-app-sepia-pi.vercel.app",
          "Access-Control-Allow-Methods": "GET, POST, OPTIONS, PUT, DELETE",
          "Access-Control-Allow-Headers": "Content-Type, Authorization"
        }
      },
      {
        "src": "/api/auth/login",
        "dest": "/api/index.js",
        "headers": {
          "Access-Control-Allow-Origin": "https://auth-app-sepia-pi.vercel.app",
          "Access-Control-Allow-Methods": "GET, POST, OPTIONS, PUT, DELETE",
          "Access-Control-Allow-Headers": "Content-Type, Authorization"
        }
      }
    ]
}
